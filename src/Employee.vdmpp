class Employee is subclass of Person
instance variables
  malfunctions:set of (Malfunction) := {};

operations
	
	public Employee: Role ==> Employee
		Employee(newRole) ==
			role := newRole;
	
	public addMalfunction: Malfunction ==> ()
		addMalfunction(malf) == 
			malfunctions := malfunctions union {malf}
		post malf in set malfunctions;
	
	public getMalfunctions: () ==> set of (Malfunction)
		getMalfunctions() ==
			return malfunctions;
end Employee
